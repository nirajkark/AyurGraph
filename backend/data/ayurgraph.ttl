@prefix ayur: <http://example.org/ayurvedic/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# Classes
ayur:Compound a rdfs:Class ; rdfs:label "Compound" .
ayur:Condition a rdfs:Class ; rdfs:label "Condition" .
ayur:Dosha a rdfs:Class ; rdfs:label "Dosha" .
ayur:Herb a rdfs:Class ; rdfs:label "Herb" .
ayur:Preparation a rdfs:Class ; rdfs:label "Preparation" .
ayur:Source a rdfs:Class ; rdfs:label "Source" .
ayur:Symptom a rdfs:Class ; rdfs:label "Symptom" .
ayur:Treatment a rdfs:Class ; rdfs:label "Treatment" .

# Properties
ayur:description a rdf:Property ; rdfs:label "Description" .
ayur:recommendedFor a rdf:Property ; rdfs:label "Recommended For" .
ayur:balancesDosha a rdf:Property ; rdfs:label "Balances Dosha" .
ayur:hasSymptom a rdf:Property ; rdfs:label "Has Symptom" .
ayur:containsHerb a rdf:Property ; rdfs:label "Contains Herb" .
ayur:treats a rdf:Property ; rdfs:label "Treats" .

# Compounds
ayur:cmp_withanolide a ayur:Compound ;
    rdfs:label "Withanolide" ;
    ayur:description "A class of naturally occurring steroids found in Ashwagandha." .

