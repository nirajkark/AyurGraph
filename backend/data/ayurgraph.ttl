# Treatments
ayur:t_detox_drink a ayur:Treatment ;
    rdfs:label "Detox Drink" ;
    ayur:containsHerb ayur:h_triphala, ayur:h_guduchi ;
    ayur:description "Formulated drinks for cleansing and detoxification." ;
    ayur:treats ayur:c_fatigue, ayur:c_toxicity .

ayur:t_herbal_tea a ayur:Treatment ;
    rdfs:label "Herbal Tea" ;
    ayur:containsHerb ayur:h_cardamom, ayur:h_tulsi, ayur:h_licorice ;
    ayur:description "Infusion/tea containing specific herbs." ;
    ayur:treats ayur:c_common_cold, ayur:c_cough .

ayur:t_meditation a ayur:Treatment ;
    rdfs:label "Meditation" ;
    ayur:description "Mind-body practice for mental clarity and relaxation." ;
    ayur:treats ayur:c_anxiety, ayur:c_insomnia .

ayur:t_oil_therapy a ayur:Treatment ;
    rdfs:label "Oil Therapy" ;
    ayur:containsHerb ayur:h_guggulu, ayur:h_sesame ;
    ayur:description "Topical oil application or massage." ;
    ayur:treats ayur:c_arthritis, ayur:c_dry_skin .

ayur:t_panchakarma a ayur:Treatment ;
    rdfs:label "Panchakarma" ;
    ayur:description "Cleansing therapies including vamana, virechana, etc." ;
    ayur:treats ayur:c_toxicity, ayur:c_indigestion .

